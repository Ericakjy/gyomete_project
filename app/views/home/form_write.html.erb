<div class="container">
  <form action="/home/form_action" method="POST">
    <div class="form-group">
      <label for="exampleInputEmail1">제목</label>
      <input name="title" type="text" class="form-control" id="exampleInputEmail1" placeholder="제목을 입력해 주세요">
    </div>
    <div class="form-group">
      <label for="comment">Comment:</label>
      <textarea name="content" class="form-control" rows="5" id="comment"></textarea>
    </div>
    <div class="form-group">
      <lable for="category">Category</label>
      <select class="form-control">
          <option value="one">성적정정</option>
          <option value="two">출석문의</option>
          <option value="three">개인질문</option>
      </select>
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
  </form>
</div>
